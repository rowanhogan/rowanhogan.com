<% date = new Date(@created_time * 1000).getTime() %>

<li class="timeline-list-item timeline-instagram" data-time="<%= date %>">
  <header>
    <label class="date"><%= moment(date).format('DD MMMM YYYY'); %></label>
    <h1 class="icon-instagram">
      <a href="<%= @link %>">Instagram</a>
    </h1>
  </header>
  <article>
    <a href="<%= @link %>" class='thumb-container' target="_blank">
      <img src="<%= @images.standard_resolution.url %>" class="thumb">
    </a>
  </article>
</li>
