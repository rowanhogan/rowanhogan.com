<li class="timeline-list-item timeline-shot icon-dribbble" data-time="<%= new Date(@created_at).getTime() %>">
  <header>
    <h1>
      <a href="<%= @url %>"><%= @title %></a>
    </h1>
  </header>
  <article>
    <a href="<%= @url %>" title="<%= @title %>" class='thumb-container' target="_blank">
      <img src="<%= @image_teaser_url %>" class="thumb">
    </a>
  </article>

</li>
